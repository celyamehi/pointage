{
  "rewrites": [
    {
      "source": "/downloads/:path*",
      "destination": "/downloads/:path*"
    },
    {
      "source": "/((?!downloads/).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/downloads/(.*).apk",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/vnd.android.package-archive"
        },
        {
          "key": "Content-Disposition",
          "value": "attachment"
        }
      ]
    }
  ]
}
